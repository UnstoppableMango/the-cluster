    "test": "jest",
    "validate": "cd ../../ && renovate-config-validator --strict"
  },
  "dependencies": {
    "renovate": "^37.31.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
